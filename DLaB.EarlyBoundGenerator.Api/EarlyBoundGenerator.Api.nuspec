    <file src="bin\Release\DLaB.EarlyBoundGenerator\alphabets\*.*" target="contentFiles\any\any\DLaB.EarlyBoundGenerator\alphabets" />
    <!-- images -->
    <file src="bin\Release\Images\DLaB_Dynamics_Logo_Square_32x32.png" target="images\" />
